
dlgp:
	wget https://github.com/mburon/guarded-saturation-experiments/releases/download/0.0.1/sanitised_cleaned_safe_ISG_DLGPs.zip -O sanitised_cleaned_safe_ISG_DLGPs.zip
	unzip sanitised_cleaned_safe_ISG_DLGPs.zip

owl:
	mkdir OWL;
	wget https://github.com/mburon/guarded-saturation-experiments/releases/download/0.0.1/ISG-cleaned-OWL.zip -O ISG-cleaned-OWL.zip
	unzip ISG-cleaned-OWL.zip

build:
	../OWL2DLGP.sh ISG-cleaned
