
dlgp:
	wget https://github.com/mburon/guarded-saturation-experiments/releases/download/0.0.1/ISG-guarded_kaon2.dlgp.zip -O ISG-guarded_kaon2.dlgp.zip
	unzip ISG-guarded_kaon2.dlgp.zip

owl:
	wget https://github.com/mburon/guarded-saturation-experiments/releases/download/0.0.1/ISG-guarded_kaon2.owl.zip -O ISG-guarded_kaon2.owl.zip
	unzip ISG-guarded_kaon2.owl.zip


build:
	../OWL2DLGP.sh ISG-guarded_kaon2
